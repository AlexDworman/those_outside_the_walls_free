randomImg":false},"items":[{"_id":"qG9wQl8qTwCaJbBa","name":"Hammer, War","type":"weapon","flags":{},"img":"systems/wwn/assets/default/weapon.png","effects":[],"system":{"range":{"short":0,"medium":0,"long":0},"save":"","pattern":"transparent","description":"<p><em><strong>Hammers&nbsp;</strong></em>listed here are the fighting variety, narrow-headed and made for penetrating or shocking heavy&nbsp;plates of armor.</p>","damage":"1d8","bonus":0,"skillDamage":false,"shock":{"damage":1,"ac":18},"shockTotal":0,"skill":"stab","score":"str","tags":[],"slow":false,"missile":false,"melee":true,"price":30,"weight":1,"counter":{"value":1,"max":1},"quantity":1,"charges":{"value":0,"max":0}},"ownership":{"default":0,"mgBwP0sTLP1VFg1m":3,"aD9eqh5kGl4155z5":3},"folder":null,"sort":0,"_stats":{"systemId":"wwn","systemVersion":"1.01.b6","coreVersion":"10.291","createdTime":1673812802238,"modifiedTime":1673812806059,"lastModifiedBy":"aD9eqh5kGl4155z5"}},{"_id":"wLuSEXgZQ9BHsSzB","name":"Mail Shirt","type":"armor","img":"systems/wwn/assets/default/armor.png","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Compendium.wwn.armor.y2XpAnL4R2KKOz0T"}},"system":{"description":"<p><em><strong>Mail shirts </strong></em>are usually of iron or steel wire, though bronze and other more exotic materials are not unknown. Such shirts cover only the vitals of the wearer, but are much less burdensome than a full hauberk.</p>\n<p><em><strong>Medium armor</strong></em> is significantly noisier and more overt; it cannot be worn discreetly and applies its encumbrance as a penalty to any physical Sneak rolls made by the wearer.</p>","ac":9,"aac":{"value":14,"mod":0},"armorBonus":0,"type":"medium","price":250,"weight":1},"ownership":{"default":0,"mgBwP0sTLP1VFg1m":3,"aD9eqh5kGl4155z5":3},"_stats":{"systemId":"wwn","systemVersion":"1.01.b6","coreVersion":"10.291","createdTime":1673812819310,"modifiedTime":1673812819310,"lastModifiedBy":"aD9eqh5kGl4155z5"}},{"_id":"sZlzMLByCnX9TmfO","name":"S